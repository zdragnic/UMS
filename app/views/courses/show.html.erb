<p id="notice"><%= notice %></p>

<% @courses = Course.where("user_id=?",session[:user_id]) %>
<% @courses.each do |course| %>

    <p>
      <strong>Title:</strong>
      <%= course.title %>
    </p>

    <p>
      <strong>Code:</strong>
      <%= course.code %>
    </p>

    <p>
      <strong>User:</strong>
      <%= course.user_id %>
    </p>
<% end %>


<%= link_to 'Edit', edit_course_path(@course) %> |
<%= link_to 'Back', courses_path %>
